<odoo>
  <data>
<record model="ir.ui.view" id="inherit_view_invoice_tree">
    <field name="name">account.view.inherit</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
         <!--Ajout du champs Référence Client dans account move line-->
        <xpath expr="//tree/field[@name='quantity']" position="before">
                <field name="client_ref" String="Customer Reference"/>
        </xpath>
    </field>
 </record>
</data>
</odoo>